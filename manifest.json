{
  "manifest_version": 2,

  "name": "SYMba",
  "description": "Chrome extension that provides full browser control and content consumption abilities through just a single click of a button.",
  "version": "1.0",

  "permissions": [

  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*", "<all_urls>"],
      "css": ["style.css"],
      "js": ["jquery-1.11.0.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
      "images/*",
      "*.js"
  ],
  "icons": { 
      "128": "images/SYMba.png" 
  }

}
